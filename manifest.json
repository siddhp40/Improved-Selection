{
  "manifest_version": 3,
  "name": "Improved Selection",
  "description": "Improves the default selection tool on all websites",
  "version": "1.0.0",
  "content_scripts": [
    {
        "matches":["<all_urls>"],
        "js": ["index.js"]
    }
  ],
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "activeTab"
  ]
}